{{#each this}}
<li class="photo-card item">
      <div class="photo-card__img-box">
            <img class="photo-card__img" src="{{webformatURL}}" alt="{{tags}}" width="{{webformatWidth}}"
                  height="{{webformatHeight}}" />
      </div>




      <ul class="photo-card__stats stats list">
            <li class="stats-item item">
                  <i class="material-icons md-18">thumb_up</i>
                  {{likes}}
            </li>

            <li class="stats-item item">
                  <i class="material-icons md-18">visibility</i>
                  {{views}}
            </li>

            <li class="stats-item item">
                  <i class="material-icons md-18">comment</i>
                  {{comments}}
            </li>

            <li class="stats-item item">
                  <i class="material-icons md-18">cloud_download</i>
                  {{downloads}}
            </li>

      </ul>
</li>
{{/each}}